{
  "name": "scipen-beamer",
  "version": "1.0.0",
  "description": "Scientific Paper to Beamer Presentation Converter - AI-powered LaTeX slide generation",
  "type": "module",
  "main": "dist/cli/index.js",
  "bin": {
    "scipen-beamer": "./dist/cli/index.js"
  },
  "scripts": {
    "build": "node build.mjs",
    "build:tsc": "tsc",
    "start": "pnpm run build && node dist/cli/index.js",
    "dev": "pnpm exec tsx src/cli/index.ts"
  },
  "keywords": [
    "scientific-paper",
    "beamer",
    "presentation",
    "latex",
    "claude",
    "ai",
    "sdk",
    "agent",
    "slides"
  ],
  "author": "SciPen AI Team",
  "license": "MIT",
  "packageManager": "pnpm@10.15.0",
  "devDependencies": {
    "@types/node": "^24.5.0",
    "esbuild": "^0.27.2",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2"
  },
  "dependencies": {
    "@anthropic-ai/claude-agent-sdk": "^0.2.21",
    "zod": "^3.24.0",
    "zod-to-json-schema": "^3.24.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
